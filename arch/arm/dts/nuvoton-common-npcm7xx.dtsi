#include <dt-bindings/clock/npcm750_poleg-clock.h>
#include <dt-bindings/gpio/gpio.h>
#include "nuvoton-npcm7xx-pinctrl.dtsi"
#include <dt-bindings/reset/nuvoton,npcm7xx-reset.h>

/ {

	#address-cells = <1>;
	#size-cells = <1>;

	clks: clock-control@f0801000 {
		compatible = "nuvoton,npcm750-clock";
		#clock-cells = <1>;
		reg = <0xf0801000 0x70>;
		u-boot,dm-pre-reloc;
	};
	
	rstc: rstc@f0801000 {
                compatible = "nuvoton,npcm750-reset";
                reg = <0xf0801000 0x78>;
                #reset-cells = <2>;
        };	

	gcr: gcr@f0800000 {
                compatible = "nuvoton,npcm750-gcr", "syscon",
                        "simple-mfd";
                reg = <0xf0800000 0x1000>;
        };

	usbphy {
                compatible = "simple-bus", "nuvoton,npcm750-usb-phy";
                #address-cells = <1>;
                #size-cells = <0>;
                syscon = <&gcr>;
		usbphy1: usbphy1 {
                        compatible = "nuvoton,npcm750-usb-phy";
                        #phy-cells = <1>;
                        reg = <1>;
                        resets = <&rstc NPCM7XX_RESET_IPSRST3 NPCM7XX_RESET_USBPHY1>;
                        status = "disabled";
                };
		usbphy2: usbphy2 {
                        compatible = "nuvoton,npcm750-usb-phy";
                        #phy-cells = <1>;
                        reg = <2>;
                        resets =<&rstc NPCM7XX_RESET_IPSRST3 NPCM7XX_RESET_USBPHY2>;
                        status = "disabled";
                };
	};
	usbd0: usbd0@f0830100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0830100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST3 NPCM7XX_RESET_UDC0>;
		status = "disabled";
	};
	usbd1: usbd1@f0831100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0831100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST1 NPCM7XX_RESET_UDC1>;
		status = "disabled";
	};
	usbd2: usbd2@f0832100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0832100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST1 NPCM7XX_RESET_UDC2>;
		status = "disabled";
	};
	usbd3: usbd3@f0833100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0833100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST1 NPCM7XX_RESET_UDC3>;
		status = "disabled";
	};
	usbd4: usbd4@f0834100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0834100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST1 NPCM7XX_RESET_UDC4>;
		status = "disabled";
	};
	usbd5: usbd5@f0835100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0835100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST1 NPCM7XX_RESET_UDC5>;
		status = "disabled";
	};
	usbd6: usbd6@f0836100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0836100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST1 NPCM7XX_RESET_UDC6>;
		status = "disabled";
	};
	usbd7: usbd7@f0837100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0837100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST3 NPCM7XX_RESET_UDC7>;
		status = "disabled";
	};
	usbd8: usbd8@f0838100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0838100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST3 NPCM7XX_RESET_UDC8>;
		status = "disabled";
	};
	usbd9: usbd9@f0839100 {
		compatible = "nuvoton,npcm750-udc";
		reg = <0xF0839100 0x200>;
		resets = <&rstc NPCM7XX_RESET_IPSRST3 NPCM7XX_RESET_UDC9>;
		status = "disabled";
	};

	serial0: serial0@f0001000 {
		compatible = "nuvoton,npcm750-uart";
		reg = <0xf0001000 0x20>;
		id = <0>;
		clocks = <&clks CLK_UART>;
		clock-frequency = <24000000>;
	};

	serial1: serial1@f0002000 {
		compatible = "nuvoton,npcm750-uart";
		reg = <0xf0002000 0x20>;
		id = <1>;
		clocks = <&clks CLK_UART>;
		clock-frequency = <24000000>;
	};

	serial2: serial2@f0003000 {
		compatible = "nuvoton,npcm750-uart";
		reg = <0xf0003000 0x20>;
		id = <2>;
		clocks = <&clks CLK_UART>;
		clock-frequency = <24000000>;
	};

	serial3: serial3@f0004000 {
		compatible = "nuvoton,npcm750-uart";
		reg = <0xf0004000 0x20>;
		id = <3>;
		clocks = <&clks CLK_UART>;
		clock-frequency = <24000000>;
	};

	timer0: timer@f0008000 {
		compatible = "nuvoton,npcm750-timer";
		reg = <0xF0008000 0x100>;
		clocks = <&clks CLK_TIMER>;
	};

	otp: otp@f0189000 {
		compatible = "nuvoton,npcm750-otp";
		reg = <0xf0189000 0x1000
			   0xf018a000 0x1000>;
		clocks = <&clks CLK_APB4>;
		status = "disabled";
	};

	rng: rng@f000b000 {
		compatible = "nuvoton,npcm750-rng";
		reg = <0xf000b000 0x1000>;
		clocks = <&clks CLK_APB1>;
		status = "disabled";
	};

	fiu0: fiu0@fb000000 {
		compatible = "nuvoton,npcm750-fiu";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0xfb000000 0x1000>;
		index = <0>;
		spi-max-frequency = <66000000>;
	};

	fiu3: fiu3@c0000000 {
		compatible = "nuvoton,npcm750-fiu";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0xc0000000 0x1000>;
		index = <3>;
		spi-max-frequency = <50000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&spi3_pins>;
		status = "disabled";
	};

	fiux: fiux@fb001000 {
		compatible = "nuvoton,npcm750-fiu";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0xfb001000 0x1000>;
		index = <4>;
		spi-max-frequency = <50000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&spix_pins>;
		status = "disabled";
	};

	pspi1: pspi1@f0200000 {
		compatible = "nuvoton,npcm750-pspi";
		#address-cells = <1>;
		#size-cells = <0>;
		reg = <0xf0200000 0x1000>;
		index = <0>;
		spi-max-frequency = <25000000>;
		cs-gpios = <&gpio6 203 GPIO_ACTIVE_HIGH>;
		clocks = <&clks CLK_APB5>;
		pinctrl-names = "default";
		pinctrl-0 = <&pspi1_pins>;
		status = "disabled";
	};

	sdhci0: sdhci0@f0840000 {
		compatible = "nuvoton,npcm750-sdhci-SD";
		reg = <0xf0840000 0x2000>;
		index = <0x0>;
		bus-width = <0x4>;
		pinctrl-names = "default";
		pinctrl-0 = <&sd1_pins>;
		status = "disabled";
	};

	sdhci1: sdhci1@f0842000 {
		compatible = "nuvoton,npcm750-sdhci-eMMC";
		reg = <0xf0842000 0x200>;
		index = <0x1>;
		bus-width = <0x8>;
		clocks = <&clks CLK_EMMC>;
		clock-frequency = <50000000>;
		pinctrl-names = "default";
		pinctrl-0 = <&mmc_pins
			&mmc8_pins>;
		status = "disabled";
	};

	aec: aes@f0858000 {
		compatible = "nuvoton,npcm750-aes";
		reg = <0xf0858000 0x1000>;
		clocks = <&clks CLK_AHB>;
		clock-names = "clk_ahb";
		status = "disabled";
	};

	sha: sha@f085a000 {
		compatible = "nuvoton,npcm750-sha";
		reg = <0xf085a000 0x1000>;
		clocks = <&clks CLK_AHB>;
		clock-names = "clk_ahb";
		status = "disabled";
	};

	gpio0: gpio0@f0010000 {
		compatible = "nuvoton,npcm750-gpio";
		reg = <0xf0010000 0x1000>;
		#gpio-cells = <2>;
		gpio-controller;
		gpio-bank-name = "gpio0";
		gpio-count = <32>;
		gpio-port = <0>;
	};

	gpio1: gpio1@f0011000 {
		compatible = "nuvoton,npcm750-gpio";
		reg = <0xf0011000 0x1000>;
		#gpio-cells = <2>;
		gpio-controller;
		gpio-bank-name = "gpio1";
		gpio-count = <32>;
		gpio-port = <1>;
	};

	gpio2: gpio2@f0012000 {
		compatible = "nuvoton,npcm750-gpio";
		reg = <0xf0012000 0x1000>;
		#gpio-cells = <2>;
		gpio-controller;
		gpio-bank-name = "gpio2";
		gpio-count = <32>;
		gpio-port = <2>;
	};

	gpio3: gpio3@f0013000 {
		compatible = "nuvoton,npcm750-gpio";
		reg = <0xf0013000 0x1000>;
		#gpio-cells = <2>;
