		gpio-count = <32>;
		gpio-port = <5>;
	};

	gpio6: gpio6@f0016000 {
		compatible = "nuvoton,npcm750-gpio";
		reg = <0xf0016000 0x1000>;
		#gpio-cells = <2>;
		gpio-controller;
		gpio-bank-name = "gpio6";
		gpio-count = <32>;
		gpio-port = <6>;
	};

	gpio7: gpio7@f0017000 {
