		gpio-count = <32>;
		gpio-port = <4>;
	};

	gpio5: gpio5@f0015000 {
		compatible = "nuvoton,npcm750-gpio";
		reg = <0xf0015000 0x1000>;
		#gpio-cells = <2>;
